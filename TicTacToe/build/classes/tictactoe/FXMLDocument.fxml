<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="200" prefWidth="320" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tictactoe.FXMLDocumentController">
    <children>
      <Button fx:id="cancelButton" layoutX="232.0" layoutY="152.0" mnemonicParsing="false" onMouseClicked="#cancelButtonClicked" text="Cancel" />
        <Button fx:id="startButton" layoutX="138.0" layoutY="152.0" onAction="#handleButtonAction" onMouseClicked="#startButtonClicked" prefHeight="25.0" prefWidth="69.0" text="START" />
      <HBox prefHeight="137.0" prefWidth="320.0">
         <children>
            <TextField fx:id="p1TextField" prefHeight="25.0" prefWidth="148.0" translateX="148.0" translateY="40.0" />
            <TextField fx:id="p2TextField" prefHeight="25.0" prefWidth="150.0" translateY="85.0" />
         </children>
      </HBox>
      <HBox prefHeight="117.0" prefWidth="138.0">
         <children>
            <Label prefHeight="34.0" prefWidth="71.0" text="Player 1" textFill="RED" translateX="49.0" translateY="35.0">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <Label prefHeight="34.0" prefWidth="71.0" text="Player 2" textFill="RED" translateX="-20.0" translateY="80.0">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
         </children>
      </HBox>
    </children>
</AnchorPane>
